{
  "displayName": "react-directus",
  "preset": "ts-jest",
  "testEnvironment": "jsdom",
  "testMatch": ["<rootDir>/src/**/*(*.)@(spec|test).[tj]s?(x)"],
  "moduleNameMapper": {
    "^@/(.*)$": "<rootDir>/src/$1",
    "^@components/(.*)$": "<rootDir>/src/components/$1",
    "^@hooks/(.*)$": "<rootDir>/src/hooks/$1",
    "^@utils/(.*)$": "<rootDir>/src/utils/$1"
  }
}
